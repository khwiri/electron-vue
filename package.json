{
  "name": "@nullkeys/electron-vue",
  "version": "2.0.1",
  "description": "Vue.js extension for Electron applications",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "coverage": "istanbul cover --report lcovonly ./node_modules/mocha/bin/_mocha",
    "coveralls": "npm run-script coverage && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullkeys/electron-vue.git"
  },
  "keywords": [
    "Vue.js",
    "Electron"
  ],
  "author": "Adam Keys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nullkeys/electron-vue/issues"
  },
  "homepage": "https://github.com/nullkeys/electron-vue#readme",
  "dependencies": {
    "electron": "^1.7.5",
    "vue": "^2.4.2",
    "vue-template-compiler": "^2.4.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "vue-server-renderer": "^2.4.2"
  }
}
